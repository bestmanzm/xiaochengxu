.page {
  width: 100%;
  background: #f7f7f7;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
}
.page .mine {
  width: 100%;
  height: 105rpx;
  line-height: 105rpx;
  background-color: #f7f7f7;
  position: absolute;
  top: 0;
  left: 0;
}
.page .mine .mine_l {
  position: absolute;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}
.page .mine .mine_l .mine_l_pic {
  width: 39rpx;
  height: 42rpx;
  position: absolute;
  top: 48%;
  transform: translateY(-50%);
}
.page .mine .mine_l .mine_l_font {
  font-size: 34rpx;
  line-height: 34rpx;
  height: 34rpx;
  width: 140rpx;
  color: #222222;
  position: absolute;
  left: 55rpx;
  top: 50%;
  transform: translateY(-50%);
}
.page .mine .mine_r {
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  color: #999999;
  font-size: 30rpx;
}
.page .mine .mine_rActive {
  color: #ff9364;
}
.page .boLang {
  width: 100%;
  height: 20rpx;
  position: absolute;
  top: 90rpx;
  left: 0;
}
.page .list {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
  background-color: #ffffff;
  position: absolute;
  top: 110rpx;
  left: 0;
}
.page .list .lost_single {
  width: 340rpx;
  height: 190rpx;
  border-radius: 6rpx;
  background-color: #ffffff;
  box-shadow: 0rpx 0rpx 20rpx #c2c2c2;
  margin-top: 40rpx;
  color: #222222;
  line-height: 36rpx;
  position: relative;
}
.page .list .lost_single .list_single_coin {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  padding: 10rpx 0;
  border-bottom: 1px dashed #222222;
}
.page .list .lost_single .lost_single_border {
  width: 332rpx;
  height: 180rpx;
  border: 1px dashed #ffffff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateY(-50%);
  margin-left: -166rpx;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.page .list .lost_single .arrow {
  width: 224rpx;
  height: 60rpx;
  position: absolute;
  left: 0;
  top: 110rpx;
}
.page .list .lost_single .arrow .arrow_bgc {
  width: 224rpx;
  height: 60rpx;
}
.page .list .lost_single .arrow .arrow_bgcNext {
  width: 224rpx;
  height: 60rpx;
  display: none;
}
.page .list .lost_single .arrow .arrow_cont {
  font-size: 40rpx;
  line-height: 40rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 20rpx;
  color: #ffffff;
}
.page .list .lost_single:last-child {
  margin-bottom: 50rpx;
}
.page .list .active {
  background-color: #ff9364;
}
.page .list .active .list_single_coin {
  color: #ffffff;
  border-bottom: 1px dashed #ffffff;
}
.page .list .active .arrow {
  width: 224rpx;
  height: 60rpx;
  position: absolute;
  left: 0;
  top: 110rpx;
}
.page .list .active .arrow .arrow_bgc {
  display: none;
  width: 224rpx;
  height: 60rpx;
}
.page .list .active .arrow .arrow_bgcNext {
  display: block;
  width: 224rpx;
  height: 60rpx;
}
.page .list .active .arrow .arrow_cont {
  font-size: 40rpx;
  line-height: 40rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 20rpx;
  color: #222222;
}
.page .boLang_next {
  width: 100%;
  height: 20rpx;
  transform: rotate(180deg);
  position: absolute;
  top: 840rpx;
  left: 0;
}
.page .instruction {
  width: 100%;
  height: 315rpx;
  position: absolute;
  top: 860rpx;
}
.page .instruction .san {
  position: absolute;
  left: 20rpx;
  top: 30rpx;
  width: 43rpx;
  height: 46rpx;
}
.page .instruction .instruction_title {
  font-size: 34rpx;
  color: #222222;
  line-height: 34rpx;
  position: absolute;
  top: 40rpx;
  left: 80rpx;
}
.page .instruction .pointOne {
  position: absolute;
  top: 110rpx;
  left: 20rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #666666;
}
.page .instruction .pointTwo {
  position: absolute;
  top: 160rpx;
  left: 20rpx;
  font-size: 30rpx;
  line-height: 42rpx;
  color: #666666;
}
